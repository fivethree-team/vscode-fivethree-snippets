{
    "fiv-item-expandable": {
        "prefix": "f-item-expandable",
        "body": [
            "<fiv-item-expandable text=\"${1:Ducks}\" icon=\"${2:search}\"", 
            "\ticonSlot=\"${3|start,end|}\" color=\"${4|primary,secondary,tertiary|}\"",
            "\t[expanded]=\"${5|true,false|}\">",
            "\t<ion-item>${6:Tick}</ion-item>",
            "\t<ion-item>${7:Trick}</ion-item>",
            "\t<ion-item>${8:Track}</ion-item>",
            "</fiv-item-expandable>"
        ],
        "description": "<fiv-item-expandable> w/ <ion-item>"
    },
    "fiv-loading-fab": {
        "prefix": "f-loading-fab",
        "body": [
            "<fiv-loading-fab checkmark=\"${1:true}\" (onComplete)=\"${2:onCompleted()}\" loading=\"${3:true}\" #fab (click)=\"fab.loading ? fab.complete() : fab.toggleSpinner()\"",
            "\thorizontal=\"${4:end}\" vertical=\"${5:bottom}\" icon=\"${6:add}\" spinColor=\"${7:dark}\" fabColor=\"${8:light}\">",
            "</fiv-loading-fab>"
        ],
        "description": "<fiv-loading-fab>"
    },
    "fiv-toolbar-search": {
        "prefix": "f-toolbar-search",
        "body": [
           "<fiv-toolbar-search (close)=\"${1:onSearchbarClose()}\" (inputChange)=\"${2:onInputChange()}\"  placeholder=\"${3:Enter search query}\" title=\"${4:Titel}\">",
           "<ion-back-button defaultHref=\"/home\" ${5|start,end|}></ion-back-button>",
           "<ion-button ${5|start,end|}>",
           "<ion-icon name=\"md-add\">",
           "</ion-button>",
       "</fiv-toolbar-search>"
        ],
        "description": "<fiv-loading-fab>"
    }
}