{
    "fiv-loading-button-click": {
        "prefix": "f-loading-button-click",
        "body": [
            "${1:login}(${2:button}: LoadingButtonComponent) {",
            "\t${2:button}.load();",
            "\t$0",
            "\t${2:button}.complete();",
            "",
            "\t${2:button}.unload();",
            "}"
        ],
        "description": "<fiv-loading-button> method for (fivButtonClick)"
    },
    "fiv-loading-content-do-refresh": {
        "prefix": "f-loading-content-do-refresh",
        "body": [
            "${1:fivRefresh}(${2:content}: FivLoadingContent) {",
            "\t$0",
            "\t${2:content}.completeRefresh();",
            "}"
        ],
        "description": "<fiv-loading-content fivRefresh> output handler method"
    }
}