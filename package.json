{
    "name": "vscode-fivethree-snippets",
    "displayName": "Fivethree Snippets",
    "description": "Fivethree Web Components Snippets for VS Code",
    "version": "0.0.11",
    "publisher": "fivethree",
    "repository": {
        "type": "git",
        "url": "https://github.com/fivethree-team/vscode-fivethree-snippets.git"
    },
    "bugs": {
        "url": "https://github.com/fivethree-team/vscode-fivethree-snippets/issues"
    },
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "fivethree",
        "Ionic",
        "Angular",
        "TypeScript",
        "Fastlane"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ruby",
                "aliases": [
                    "Ruby",
                    "ruby"
                ],
                "filenamePatterns": [
                    "Appfile",
                    "Deliverfile",
                    "Fastfile",
                    "Gymfile",
                    "Matchfile",
                    "Scanfile",
                    "Snapfile",
                    "Podfile",
                    "*.podspec",
                    "Fastfile*",
                    "*Fastfile"
                ]
            }
        ],
        "snippets": [
            {
                "language": "ruby",
                "path": "./snippets/fastlane.json"
            },
            {
                "language": "ruby",
                "path": "./snippets/fastlane-fivethree.json"
            },
            {
                "language": "html",
                "path": "./snippets/html.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/typescript.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "version": "conventional-changelog -p angular -i ./CHANGELOG.md -s"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.26",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}